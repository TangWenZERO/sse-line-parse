# NPM configuration for sse-stream-parser

# Registry configuration
registry=https://registry.npmjs.org/

# Always save with exact versions
save-exact=true

# Do not audit production dependencies automatically
audit=false

# Do not send audit report to npm registry
audit-level=none

# Package lock behavior
package-lock=true
package-lock-only=false

# Save behavior
save-prefix=~

# Strict peer dependencies enforcement
strict-peer-deps=false

# Engine strictness
engine-strict=false

# Scripts
ignore-scripts=false

# Prefer offline mode
prefer-offline=true